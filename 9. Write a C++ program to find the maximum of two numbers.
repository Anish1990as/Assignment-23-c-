// 9. Write a C++ program to find the maximum of two numbers.
#include<iostream>
using namespace std;
int main()
{
    int x,y;
    cout<<"enter two number"<<endl;
    cin>>x>>y;
    if(x>y)
        cout<<"Maximum number is : "<<x;
    else if(y>x)
        cout<<"Maximum number is : "<<y;
    else
        cout<<"both are equal";
    return 0;

}
